# アプリケーション設定
LLMEVAL_ENV=development
LLMEVAL_LOG_LEVEL=INFO

# データパス設定
LLMEVAL_DATASET_DIR=/path/to/datasets/test
LLMEVAL_TRAIN_DIR=/path/to/datasets/train
LLMEVAL_RESULTS_DIR=/app/results

# モデル設定
LLMEVAL_LITELLM_BASE_URL=http://localhost:11434/api/generate
LLMEVAL_DEFAULT_MAX_TOKENS=1024
LLMEVAL_DEFAULT_TEMPERATURE=0.0
LLMEVAL_DEFAULT_TOP_P=1.0

# 処理設定
LLMEVAL_BATCH_SIZE=5
LLMEVAL_DEFAULT_NUM_SAMPLES=10
